@model Ads.Models.AdminMessage

@{
    ViewBag.Title = @Ads.Resources.Texts.Details;

}

<h2>@Ads.Resources.Texts.Details</h2>

<div>
    <h4>@Ads.Resources.Texts.AdminMsg</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Ads.Resources.Texts.Title
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Title)
        </dd>

        <dt>
            @Ads.Resources.Texts.Content
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Content)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink(@Ads.Resources.Texts.Edit, "Edit", new { id = Model.AdminMessageID }) |
    @Html.ActionLink(@Ads.Resources.Texts.BackToList, "Index")
</p>
